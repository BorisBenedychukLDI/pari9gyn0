<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yellow_theme_9gyn2"
    tools:context=".MainActivity9gyn0">

    <Button
        android:id="@+id/b_9gyn0"
        android:layout_width="53dp"
        android:layout_height="48dp"
        android:layout_margin="30dp"
        android:background="@drawable/custom_button_9gyn0"
        android:elevation="0dp"
        android:textAllCaps="false"
        android:textColor="@color/yellow_theme_9gyn2"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_logo_9gyn0"
        android:layout_width="212dp"
        android:layout_height="102dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:contentDescription="@string/pari_logo_9gyn0"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_9gyn0" />

    <ProgressBar
        android:id="@+id/pr_9gyn0"
        style="?android:attr/progressBarStyle"
        android:layout_width="33dp"
        android:layout_height="30dp"
        android:elevation="20dp"
        android:indeterminateTint="@color/yellow_theme_9gyn2"
        app:layout_constraintBottom_toBottomOf="@+id/b_9gyn0"
        app:layout_constraintEnd_toEndOf="@+id/b_9gyn0"
        app:layout_constraintStart_toStartOf="@+id/b_9gyn0"
        app:layout_constraintTop_toTopOf="@+id/b_9gyn0" />

    <TextView
        android:id="@+id/tv_countDown_9gyn0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black_9gyn0"
        android:textSize="96sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>